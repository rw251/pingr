{
  "name": "pingr",
  "version": "0.0.1",
  "description": "The performance improvement plan generator",
  "main": "script.js",
  "scripts": {
    "start": "brunch watch --server",

    "build": "brunch build --production",
    "postbuild": "cp -r dist/* dist-test/" ,

    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "jshint --show-non-errors script.js data.json",

    "predeploy": "cd dist && git pull origin gh-pages",
    "deploy": "npm run build",
    "postdeploy" :"cd dist && git add -A && git commit -m \"Latest test version\" && git push origin gh-pages",

    "predeploy:test": "cd dist-test && git pull origin gh-pages",
    "deploy:test": "npm run build",
    "postdeploy:test" :"cd dist-test && git add -A && git commit -m \"Latest test version\" && git push origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rw251/rw251.github.io.git"
  },
  "author": "Richard Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rw251/rw251.github.io/issues"
  },
  "homepage": "https://github.com/rw251/rw251.github.io",
  "devDependencies": {
    "auto-reload-brunch": "^2.7.1",
    "browserify": "^13.0.0",
    "brunch": "^2.8.0",
    "cp": "^0.2.0",
    "csv-parser": "^1.9.3",
    "http-server": "^0.8.0",
    "jade-brunch": "^2.8.0",
    "javascript-brunch": "^2.0.0",
    "jshint": "^2.7.0",
    "mv": "^2.0.3",
    "nodemon": "^1.9.1",
    "rimraf": "^2.3.4",
    "sass-brunch": "^2.6.3",
    "uglify-js": "^2.6.2"
  },
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "highcharts": "^4.2.4",
    "jquery-sparkline": "^2.3.2",
    "mustache": "^2.2.1",
    "zeroclipboard": "^2.2.0"
  }
}
