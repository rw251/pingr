{
  "name": "pingr",
  "version": "0.0.1",
  "description": "The performance improvement plan generator",
  "main": "script.js",
  "scripts": {
    "prestart": "rimraf dist/*",
    "start": "brunch watch --server",
    "prebuild": "ncp node_modules/zeroclipboard/dist/ZeroClipboard.swf ./app/assets/ZeroClipboard.swf",
	"testing": "brunch watch --server --debug",
    "build": "brunch build --production",
    "postbuild": "ncp deploy/appserver/.foreverignore dist/.foreverignore",
    "postinstall": "npm run build",
    "watch": "brunch watch",
    "predebug": "echo In one window launch brunch without server: npm run watch",
    "debug": "echo Then in second window launch server with debug: node --inspect --debug-brk server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "jshint --show-non-errors app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rw251/rw251.github.io.git"
  },
  "author": "Richard Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rw251/rw251.github.io/issues"
  },
  "homepage": "https://pingr-demo.herokuapp.com",
  "devDependencies": {
    "auto-reload-brunch": "^2.7.1",
    "brunch": "^2.8.2",
    "cp": "^0.2.0",
    "jade-brunch": "^2.8.0",
    "javascript-brunch": "^2.0.0",
    "jshint": "^2.7.0",
    "ncp": "^2.0.0",
    "rimraf": "^2.3.4",
    "sass-brunch": "^2.6.3",
    "uglify-js": "^2.6.2"
  },
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "connect-flash": "^0.1.1",
    "csv-parser": "^1.9.3",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "forever": "^0.15.3",
    "highcharts": "^4.2.4",
    "jade": "^1.11.0",
    "jquery-sparkline": "^2.3.2",
    "json2csv": "^3.7.1",
    "mongoose": "^4.6.4",
    "morgan": "^1.7.0",
    "nodemailer": "^2.6.4",
    "nodemailer-smtp-transport": "^2.7.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
