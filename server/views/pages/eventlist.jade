include ../partials/head.jade
.container.starter-template-high
  .btn-toolbar
    a.btn.btn-info(href='/') Back to interface
  table.table
    thead
      tr
        th User
        th Session
        th Event
        th Url
        th Time
        th Data
    tbody
      each event in events
        tr
          td= event.user
          td= event.sessionId
          td= event.type
          td= event.url
          td= event.date
          td= event.data.length>0 ? event.data : ''
